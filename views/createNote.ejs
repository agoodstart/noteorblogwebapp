<form action="/note/create" method="POST">
<div class="form-group">
    <label for="noteTitle"></label>
    <input type="text" id="noteTitle" name="noteTitle" placeholder="Title"class="form-control" >
</div>

<div class="form-group">
    <label for="noteBody"></label>
    <textarea rows="5" cols="30" id="noteBody", name="noteBody", placeholder="Body"></textarea>
</div>

<button type="submit" class="btn btn-primary brtn-block">Save note</button>
<a href="/dashboard" class="btn btn-secondary btn-block">Back to dashboard</a>
</form>

    