<form action="" method="" class="AJAXForm" name="<%= noteTitle %>">
<div class="form-group">
    <label for="noteTitle"></label>
    <input type="text" id="<%= _id %>" name="noteTitle" placeholder="Title" class="form-control" value="<%= noteTitle %>" />
</div>

<div class="form-group">
    <label for="noteBody"></label>
    <textarea rows="5" cols="30" id="<%= _id %>" name="noteBody" placeholder="Body"><%= noteBody %></textarea>
</div>
<%# how I change the requestheader %> 
<button type="submit" class="AJAXUpdate btn btn-primary brtn-block">Update</button>
<button type="submit" formaction="/dashboard/note/<%= _id %>?_method=DELETE" class="btn btn-primary brtn-block">Delete</button>
</form>

    